{
  "name": "docsify-slides",
  "version": "1.1.0",
  "description": "A docsify plugin for creating slideshow-like pages.",
  "main": "src",
  "repository": "https://github.com/shawntabrizi/docsify-slides",
  "author": "Shawn Tabrizi <shawntabrizi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rollup -c -w",
    "clean": "rimraf ./dist",
    "prebuild": "run-s clean",
    "build": "run-s build:*",
    "build:rollup": "rollup -c",
    "build:minify": "uglifyjs --compress --mangle --output ./dist/docsify-slides.min.js -- ./dist/docsify-slides.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "cssnano": "^4.1.10",
    "deepmerge": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.79.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^2.0.5",
    "uglify-js": "^3.3.20"
  },
  "keywords": [
    "docsify",
    "slides",
    "slideshow",
    "presentation"
  ],
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
